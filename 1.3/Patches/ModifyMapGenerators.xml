<?xml version="1.0" encoding="utf-8"?>

<Patch>

	<Operation Class="PatchOperationSequence">
		<success>Always</success>
		<operations>
			
			<li Class="PatchOperationReplace">
				<xpath>/Defs/GenStepDef[defName = "RocksFromGrid"]/genStep</xpath>
				<value>
					<genStep Class="CSG.GenStep_StoneFromGrid"/>
				</value>
			</li>
			
			<li Class="PatchOperationReplace">
				<xpath>/Defs/GenStepDef[defName = "RocksFromGrid_NoMinerals"]/genStep</xpath>
				<value>
					<genStep Class="CSG.GenStep_StoneFromGrid"/>
				</value>
			</li>
			
		</operations>
	</Operation>
	
</Patch>